{
  "mcpServers": {
    "duckduckgo": {
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/duckduckgo"
      ],
      "command": "docker",
      "disabled": false
    },
    "fetch": {
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/fetch"
      ],
      "command": "docker",
      "disabled": false
    },
    "markdownify": {
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/local-directory:/local-directory",
        "mcp/markdownify"
      ],
      "command": "docker",
      "disabled": false
    },
    "memory": {
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/local-directory:/local-directory",
        "mcp/memory"
      ],
      "command": "docker",
      "disabled": false
    },
    "nixos": {
      "args": [
        "run",
        "--rm",
        "-i",
        "ghcr.io/utensils/mcp-nixos"
      ],
      "command": "docker",
      "disabled": false
    },
    "notion": {
      "disabled": false,
      "url": "https://mcp.notion.com/mcp"
    },
    "sequentialthinking": {
      "args": [
        "run",
        "--rm",
        "-i",
        "mcp/sequentialthinking"
      ],
      "command": "docker",
      "disabled": false
    },
    "terraform": {
      "args": [
        "run",
        "-i",
        "--rm",
        "hashicorp/terraform-mcp-server"
      ],
      "command": "docker",
      "disabled": false
    },
    "time": {
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/time"
      ],
      "command": "docker",
      "disabled": false
    },
    "youtube_transcript": {
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/youtube-transcript"
      ],
      "command": "docker",
      "disabled": false
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "custom_instructions": {
        "disabled": false
      }
    }
  }
}
